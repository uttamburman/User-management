<!-- Add Modal  -->

<div id="addEmployeeModal"  class="modal fade" data-backdrop="static" >
    <div class="modal-dialog">
        <div class="modal-content">
            <form action="/add/emp_details/tbl02/"  class="was-validated" role="form" novalidate=""  method="POST">
                <div class="modal-header">						
                    <h4 class="modal-title">Create User</h4>
                    <button type="button" class="close" data-dismiss="modal"  aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label>ID</label>
                        <select  for = "empId"  type="combobox" id="empdataId01"   class="form-control submitform" name="empId" required> 
                            <option value="" disabled selected>Select Employee ID . .</option>
                            <% lst_Allemp.forEach(function(item , index){%>
                            <option value="<%= item.emp_Id  %>"><%= item.emp_Id %></option>
                            <%  }); %>
                            </select>
                    </div>					
                    <div class="form-group">
                        <label>Name</label>
                        <input type="text"  name="empName" class="form-control" required readonly="true">
                    </div>
                    <div class="form-group">
                        <label>ISID</label>
                        <input type="text"  name="empisid"  oninput="this.value = this.value.toUpperCase()" class="form-control" required readonly="true">
                    </div>
                    <div class="form-group">
                        <label>Email</label>
                        <input type="email"  name="empEmail" class="form-control" required readonly="true" >
                    </div>	
                    <div class="form-group">
                        <label>Roles</label>
                    <select multiple for = "emprole"   type="combobox"  id="addroleId01"   class="form-control submitform" name="emproles" required> 
                        <option value="" disabled selected>Select Role . .</option>
                        <% lst_usrRoles.forEach(function(item , index){%>
                        <option value="<%= item %>"><%= item %></option>
                        <%  }); %>
                        </select>	
                    </div>				
                </div>
                <div class="modal-footer">
                    <div class="pull-right">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-success" >Add</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>

<!-- Edit Modal HTML -->
<div id="editEmployeeModal" class="modal fade" data-backdrop="static" >
    <div class="modal-dialog">
        <div class="modal-content">
            <form id="Tableupdatesubmit"action="" class="was-validated" method="POST">
                <div class="modal-header">						
                    <h4 class="modal-title">Edit Employee</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label>ID</label>
                            <input list="browsers"  for = "empId02" class="form-control submitform"  id="empdataId02" name="empId02" disabled/>
                            <datalist id="browsers">
                                <% lst_Allemp.forEach(function(item , index){%>
                                    <option value="<%= item.emp_Id  %>"><%= item.emp_Id %></option>
                                    <%  }); %>
                              </datalist>
                    </div>					
                    <div class="form-group">
                        <label>Name</label>
                        <input type="text"id="empNameId"   name="empName02" class="form-control" disabled>
                    </div>
                    <div class="form-group">
                        <label>ISID</label>
                        <input type="text"  id="empISIDData"   oninput="this.value = this.value.toUpperCase()" name="empISID02data" class="form-control" disabled>
                    </div>
                    <div class="form-group">
                        <label>Email</label>
                        <input type="email" id ="empEmailId" name="empEmail02" class="form-control" disabled>
                    </div>	
                    <div class="form-group">
                        <label>Roles</label>
                    <select  multiple for = "emproles02"  type="combobox"  id="addroleId02"  autocomplete="off" class="form-control submitform" name="emproles02" required> 
                        <option value="" disabled selected>Select Role . .</option>
                        <% lst_usrRoles.forEach(function(item , index){%>
                        <option value="<%= item %>"><%= item %></option>
                        <%  }); %>
                        </select>
                    </div>						
                </div>
                <div class="modal-footer">
                    <div class="pull-right">
                    <button type="button" class="btn btn-default" data-dismiss="modal" value="Cancel">Cancel</button>
                    <button type="submit" class="btn btn-info" value="Save">Save</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
